{
  "name": "git-tag-check-action",
  "version": "1.0.18",
  "private": true,
  "description": "Check if the provided git tag exists.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "test:clear": "jest --clearCache",
    "all": "npm run format && npm run lint && npm run build && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbonkr/git-tag-check-action.git"
  },
  "keywords": [
    "actions",
    "github",
    "git",
    "git tag"
  ],
  "author": {
    "email": "dev@bbon.kr",
    "name": "Pon Cheol Ku (구본철)",
    "url": "https://bbon.kr"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1"
  },
  "devDependencies": {
    "@octokit/plugin-rest-endpoint-methods": "^9.0.0",
    "@octokit/types": "^11.1.0",
    "@types/node": "^20.4.2",
    "@typescript-eslint/parser": "^5.62.0",
    "@vercel/ncc": "^0.36.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-plugin-github": "^4.9.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.4",
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
